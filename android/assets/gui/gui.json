{
	"com.badlogic.gdx.graphics.g2d.BitmapFont": { // Fonts
		"standard": { "file": "fonts/tahoma.fnt" },
	},
	"com.badlogic.gdx.graphics.Color": { // Colors
		"white": { "r": 1, "g": 1, "b": 1, "a": 1 },
		"black": { "r": 0, "g": 0, "b": 0, "a": 1 },
	},
	"com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle": { // Label styles
		"default": { "font": standard , "fontColor": white },
	},
	"com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle": { // Button styles
		"default": { "up": blue_button_up , "down": blue_button_down, "font": standard, "pressedOffsetY": -4 }
	},
	"com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle": { // CheckBox styles
		"default": { "checkboxOn": blue_boxCheck, "checkboxOff": boxEmpty, "font": standard, "fontColor": white }
	}
}